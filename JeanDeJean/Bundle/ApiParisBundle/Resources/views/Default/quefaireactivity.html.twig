{% extends 'JeanDeJeanApiParisBundle::demo.html.twig' %}
{% block header_page %}
<div class="page-header">
    <h1>Api {Paris}</h1>
    <p class="text-info">Que Faire</p>
</div>
{% endblock %}
{% block body %}
    {% for elem in activity.data %}
    <div class="page-header">
        <p class="text-info">{{ elem.nom|raw }}</p>
    </div>
    <span>id: {{ elem.idactivites }}</span>
    <p>{{ elem.description|raw }}</p>
    {% endfor %}
{% endblock %}

